{
    "name": "TaskList",
    "description": "Build an Extension TaskList",
    "version": "1.0",
    "manifest_version": 3,

    "permissions": [
      "storage"
    ],
  
    "action": {
      "default_icon": {
        "16": "/images/icon-16.png",
        "32": "/images/icon-32.png",
        "128": "/images/icon-128.png"
      },
      "default_popup": "popup.html"
    },
    
    "icons": {
      "16": "/images/icon-16.png",
      "32": "/images/icon-32.png",
      "128": "/images/icon-128.png"
    },

    "options_page": "options.html"
    
  }